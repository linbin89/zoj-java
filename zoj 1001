import java.util.*;

public class z1001 {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		System.out.println("请输入两个整数a和b：");
		int a = sc.nextInt();
		int b = sc.nextInt();
		System.out.println("a:" + a + "  " + "b:" + b);
		System.out.println("a+b=" + (a + b));

	}
}
